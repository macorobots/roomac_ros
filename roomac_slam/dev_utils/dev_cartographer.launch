<launch>
  <arg name="rosbag" default="lap2-23-07-filtered-vo.bag"/>
  <arg name="rosbag_play_rate" default="2"/>

  <param name="use_sim_time" type="bool" value="true" />

  <node pkg="rosbag" type="play" name="player" output="screen" args="--clock -r $(arg rosbag_play_rate) /home/roomac/rosbags/$(arg rosbag)"/>

  <param name="robot_description" command="$(find xacro)/xacro $(find roomac_description)/urdf/roomac.xacro" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

  <include file="$(find roomac_slam)/launch/cartographer_vo.launch" />

  <include file="$(find roomac_slam)/launch/rviz_cartographer.launch" />

  <include file="$(find rgbd_launch)/launch/kinect_frames.launch" />

  <node name="odom_tf_to_topic" pkg="roomac_slam" type="odom_tf_to_topic.py"/>

</launch>