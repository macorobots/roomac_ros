<launch>

<rosparam file="$(find roomac_description)/launch/controller.yaml" command="load"/>

<node args="arm_position_controller joint_state_controller" name="controller_spawner" ns="/roomac" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>

<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/roomac/joint_states"/>
</node>

</launch>