<?xml version="1.0" ?>
<robot name="roomac" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="gripper_right_grip"/>
  <joint name="right_grip" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.0 -0.04"/>
    <parent link="gripper_right_1"/>
    <child link="gripper_right_grip"/>
  </joint>


  <link name="gripper_right_1">
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.01"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.01"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <mass value="0.4"/>
      <inertia
        ixx="0.05" ixy="0.0" ixz="0.0"
        iyy="0.002" iyz="0.0"
        izz="0.05"/>
    </inertial>
  </link>

  <joint name="finger1_joint" type="prismatic">
    <parent link="gripper_right_1"/>
    <child link="finger1"/>
    <origin xyz="0 0.05 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" velocity="100" lower="-0.05" upper="0.0"/>
  </joint>

  <link name="finger1">
    <collision>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.06" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.06" />
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia
        ixx="0.02" ixy="0.0" ixz="0.0"
        iyy="0.02" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>

  <joint name="finger2_joint" type="prismatic">
    <parent link="gripper_right_1"/>
    <child link="finger2"/>
    <origin xyz="0 -0.05 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="100" velocity="100" lower="0.0" upper="0.05"/>
  </joint>

  <link name="finger2">
    <collision>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.06" />
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.06" />
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 -0.03" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia
        ixx="0.02" ixy="0.0" ixz="0.0"
        iyy="0.02" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>
</robot>