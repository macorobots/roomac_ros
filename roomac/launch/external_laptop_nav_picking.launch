<launch>
  <arg name="picking_correction_enabled" default="false" />

  <include file="$(find roomac_description)/launch/description.launch" />
  <include file="$(find roomac_kinect)/launch/upper_kinect.launch" />
  <include file="$(find roomac_ar_tag)/launch/roomac_ar_tag.launch" />
  <include file="$(find roomac_moveit)/launch/roomac_moveit.launch" />
  <include file="$(find roomac_autonomous_manipulation)/launch/roomac_autonomous_manipulation.launch">
    <arg name="simulation" value="false" />
    <arg name="picking_correction_enabled" value="false" />
  </include>

  <arg name="rvizconfig" default="$(find roomac_move_base)/rviz/rviz_move_base.rviz" />
  <node name="rviz_move_base" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" />
</launch>